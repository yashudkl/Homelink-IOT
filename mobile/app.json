{
  "expo": {
    "name": "HomeLink",
    "slug": "homelink-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "scheme": "homelink-mobile",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a1a0a"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#0a1a0a"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.kathmanduuniversity.homelink"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      "expo-web-browser",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "d65c20b4-a207-40a6-b71b-81ecd346455e"
      }
    }
  }
}
